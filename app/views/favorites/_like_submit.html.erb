<% if current_user.cannot_push?(object) %>
  <div class='sent'><%= current_user.like_status(object) %></div> 
<% else %>
  <%= form_with(url: like_relationships_path, local: true) do |f| %>
    <div><%= hidden_field_tag :like_receiver_id, object.id %></div>
    <%= f.submit current_user.like_status(object), class: "likebtn" %>
  <% end %>
<% end %>
<%= form_with(url: favorites_path, local: true) do |f| %>
  <div><%= hidden_field_tag :favorite_user_id, object.id %></div>
  <%= f.submit 'favorite!', class: "likebtn" %>
<% end %>