<header class='header'>
  <%= link_to "Time Matcher", root_path, id: "logo" %>
  <div class='header-content'>
    <% if logged_in? %>
      <%= link_to "Find", list_user_path(current_user) %>
      <%= link_to "Matchers", rooms_path %>
      <%= link_to "Profile", current_user %>
      <%= link_to "Log out", logout_path, method: :delete %>
    <% else %>
      <%= link_to "Log in", login_path %>
    <% end %>
  </div>
</header>